{
  "manifest_version": 2,
  "name": "are.na new tab",
  "description": "Opens a new tab using an are.na image",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["js/newtab.js"]
  }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
