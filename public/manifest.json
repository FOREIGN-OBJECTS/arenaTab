{
  "manifest_version": 2,
  "name": "arenaTab",
  "author": "Kalli Retzepi",
  "description": "Transform a new tab into an are.na channel gallery. A FOREIGN OBJECTS side project.",
  "version": "1.1",
  "homepage_url": "https://github.com/Kallirroi/arenaTab",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["js/newtab.js"]
  }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
